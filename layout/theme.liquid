    <!-- <a href="/cart">cart</a> -->
  
    {% if shop.customer_accounts_enabled %}
      {% if customer %}
        <a href="/account">account</a>
        {{ 'log out'  | customer_logout_link }}
      {% else %}
        {{ 'log in ' | customer_login_link }}
        {{ 'register' | customer_register_link }}
      {% endif %}
    {% endif %}
  </div>

  <main role="main">
    {{ content_for_layout }}
  </main>

  <script type='text/javascript'>
    $(document).ready(function() {

      const { createApp } = Vue
      const { 
        mapActions, 
        mapGetters 
      } = Vuex;

      createApp({
        delimiters: ['${', '}'],
        data: () => ({
          header: "Cabe√ßalho"
        }),
        methods: {
          ...mapActions([
            "setHeader",
          ]),
        },
        computed: {
          ...mapGetters([
            "getHeader",
          ])
        },
      })
      .use(store)
      .mount("#app")

      $('.sidenav').sidenav();

    });
  </script>
</body>
</html>
