      $('.sidenav').sidenav();

      let mainEl = document.querySelector('main');
      $('.container-scroll-to-top a').click(
        function (e) {
          $('main').off('scroll')
          scrollTo(0)
          $('main').scroll(
            function (e) {
              if (mainEl.scrollTop < 300) {
                $scrollToTopButton.css("display", 'none')
              } else {
                $scrollToTopButton.css("display", 'flex')
              }
            }
          )
        }
